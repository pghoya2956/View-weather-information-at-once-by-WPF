<Application x:Class="View_weather_information_at_once.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModel="clr-namespace:View_weather_information_at_once.ViewModel"
             xmlns:view="clr-namespace:View_weather_information_at_once.View"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
			</ResourceDictionary.MergedDictionaries>

			<!--OverViewViewModel 타입의 내용을 OverviewView 형식으로 표현해라 -->
			<DataTemplate DataType="{x:Type viewModel:OverViewViewModel}">
				<view:OverviewView/>
			</DataTemplate>

		</ResourceDictionary>

    </Application.Resources>
</Application>
